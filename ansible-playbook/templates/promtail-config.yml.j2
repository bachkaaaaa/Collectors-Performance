server:
  http_listen_port: 9080
  grpc_listen_port: 0

positions:
  filename: /tmp/positions.yaml

clients:
  - url: http://your-loki-endpoint/loki/api/v1/push

scrape_configs:
- job_name: test_log
  static_configs:
  - targets:
      - localhost
    labels:
      job: test_log
      __path__: {{ log_file_path }}
